{
    "version": "1.5.0-alpha",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "./**/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "./src/db.ts",
        "./src/main.ts",
        "./src/router.ts",
        "./src/socket.ts",
        "./src/world.ts",
        "./typings/express/express.d.ts",
        "./typings/mime/mime.d.ts",
        "./typings/nedb/nedb.d.ts",
        "./typings/node/node.d.ts",
        "./typings/serve-static/serve-static.d.ts",
        "./typings/socket.io/legacy/socket.io-0.9-tests.ts",
        "./typings/socket.io/legacy/socket.io-0.9.d.ts",
        "./typings/socket.io/socket.io-tests.ts",
        "./typings/socket.io/socket.io.d.ts"
    ],
    "compileOnSave": false
}
